{
    "name": "laravel-hyperf/process",
    "description": "The process package for Laravel Hyperf.",
    "license": "MIT",
    "keywords": [
        "php",
        "hyperf",
        "process",
        "swoole",
        "laravel-hyperf"
    ],
    "support": {
        "issues": "https://github.com/laravel-hyperf/components/issues",
        "source": "https://github.com/laravel-hyperf/components"
    },
    "authors": [
        {
            "name": "Albert Chen",
            "email": "albert@laravel-hyperf.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel-hyperf/support": "^0.1",
        "hyperf/collection": "^3.1",
        "hyperf/macroable": "^3.1",
        "hyperf/tappable": "^3.1",
        "symfony/process": "^6.2"
    },
    "autoload": {
        "psr-4": {
            "LaravelHyperf\\Process\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "0.1-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
